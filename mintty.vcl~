# Voice commands for mintty
package install = "apt-cyg install ";
package remove = "apt-cyg remove ";


include Unimacro.vch;

# Voice commands for putty

# screen

screen next = {Ctrl + a} {n};
screen prev = {Ctrl + a} {p};
screen 0..9 = {Ctrl + a} {$};
#screen detach = {Ctrl + T} {d};
#screen attach = "tmux attach {enter}";
#screen reattach = "tmux attach -d {enter}";

# symbols

#open = {Ctrl+X}{Ctrl+F};
#save = {Ctrl+X}{Ctrl+S};
#center = {Ctrl+L};
#menu = {Ctrl+X}{Ctrl+B};
#direct = {Ctrl+X}{d};
#kaill = {Ctrl+X}{k};
#up = {Ctrl+b};
#down = {Ctrl+f};
#out = {Esc};
#jump = " ";
#quit = {Ctrl+G};
#switch = {Ctrl + X} {o};
#split = {Ctrl +X} {3};
slap = {enter};
#in = {insert};
back = {backspace};
#search = {ctrl +s};
hat = "^";

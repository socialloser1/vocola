# keyboard mappings for "screen" unix tool

previous = {Ctrl+a}p;
next = {Ctrl+a}n;
new = {Ctrl+a}c;
remove = {Ctrl+a}X;
crop = {Ctrl+a}Q;
scrollback mode = {Ctrl+a}[;
scrollback paste = {Ctrl+a}];
scrollback mark = {'>'} ;
kill = {Ctrl+a}k;
help = {Ctrl+a}?;
split = {Ctrl+a}S;
title = {Ctrl+a}A;
list = {Ctrl+a}";
switch = {Ctrl+a}{tab};
detach = {Ctrl+a}d;

